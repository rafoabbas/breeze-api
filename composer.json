{
    "name": "stephenjude/breeze-api",
    "description": "Minimal Laravel API authentication scaffolding with Sanctum.",
    "keywords": ["laravel", "auth", "api"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/stephenjude/breeze-api/issues",
        "source": "https://github.com/stephenjude/breeze-api"
    },
    "authors": [
        {
            "name": "Stephen Jude",
            "email": "hello@stephenjude.me"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/filesystem": "^8.42",
        "illuminate/support": "^8.42",
        "illuminate/validation": "^8.42"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\BreezeApi\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravel\\BreezeApi\\BreezeApiServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
